{
    "manifestVersion": 1,
    "id": "semantic-release",
    "name": "Semantic Release",
    "version": "0.0.0",
    "publisher": "danielhabenicht",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "galleryFlags": [
        "Preview"
    ],
    "public": true,
    "description": "Semantic Release in one task.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/DanielHabenicht/AzurePipelines-SemanticRelease/wiki"
        },
        "support": {
            "uri": "https://github.com/DanielHabenicht/AzurePipelines-SemanticRelease/issues/new"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/DanielHabenicht/AzurePipelines-SemanticRelease"
    },
    "files": [
        {
            "path": "freestyle"
        }
    ],
    "contributions": [
        {
            "id": "tasks",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "freestyle"
            }
        }
    ]
}